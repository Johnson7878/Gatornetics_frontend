generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity", "fullTextSearch", "fullTextIndex"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model players {
  id           Int              @id
  team_id      Int?
  firstname    text
  lastname     text
  team         text
  year         Int
  position     text
  jerseynumber text
  height       Int?
  weight       Int?
  homecity     text
  homestate    text
  posstat1     Json?
  posstat2     Json?
  conference   text
  imglinx      text
  isreturning  Int?
  regposstat1  Json?
  regposstat2  Json?

  @@index([team_id], map: "teamID")
  @@fulltext([firstName])
  @@fulltext([firstName, lastName])
  @@fulltext([lastName])
}

model teams {
